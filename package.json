{
  "name": "todo-elm-app",
  "private": true,
  "scripts": {
    "postinstall": "elm-tooling install",
    "build": "npx elm make --output dist/elm.js src/Main.elm",
    "dev": "npx elm-live src/Main.elm --path-to-elm=./node_modules/.bin/elm --start-page=index.html --port=3000 --hot -- --output=./dist/elm.js"
  },
  "devDependencies": {
    "elm-live": "^4.0.2",
    "elm-tooling": "^1.15.0"
  }
}
